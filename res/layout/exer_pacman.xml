<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/exer_pacman_base_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >
    
    <ImageButton
        android:id="@+id/exer_pacman_settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/exer_pacman_settings"
        android:background="@android:color/transparent" />
    <ImageButton
        android:id="@+id/exer_pacman_tutorials"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/exer_pacman_ranking"
        android:background="@android:color/transparent"
        android:layout_gravity="right" />
    <WebView
        android:id="@+id/exer_pacman_web_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center|top"
        android:background="@android:color/transparent" />
    
</FrameLayout>