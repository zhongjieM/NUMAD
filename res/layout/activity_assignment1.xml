<?xml version="1.0" encoding="utf-8"?>
<!--  -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:gravity="bottom"
    android:orientation="vertical"
    android:padding="30dip"
    tools:context=".Assignment1" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical" >

        <Button
            android:id="@+id/btn_team_members"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/btn_team_member_activity_assignment1" />

        <Button
            android:id="@+id/btn_sudoku_game"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/btn_sudoku_activity_assignment1" />

        <Button
            android:id="@+id/btn_boggle_game"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/btn_boggle_game" />

        <Button
            android:id="@+id/btn_persistent_boggle"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/btn_persistent_boggle" />

        <Button
            android:id="@+id/btn_tricky"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Trickiest Part." />

        <Button
            android:id="@+id/btn_final"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Final Project" />

        <Button
            android:id="@+id/btn_error"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/btn_error_check_activity_assignment1" />

        <Button
            android:id="@+id/btn_exit"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/btn_exit_activity_assignment1" />
    </LinearLayout>

</RelativeLayout>