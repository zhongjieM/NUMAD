<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="30dip"
    tools:context=".Assignment1"
    android:background="@drawable/background_landscape" >

    <Button
        android:id="@+id/btn_sudoku_game"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_team_members"
        android:layout_centerHorizontal="true"
        android:text="@string/btn_sudoku_activity_assignment1" />

    <Button
        android:id="@+id/btn_error"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_sudoku_game"
        android:layout_centerHorizontal="true"
        android:text="@string/btn_error_check_activity_assignment1" />

    <Button
        android:id="@+id/btn_exit"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_error"
        android:layout_centerHorizontal="true"
        android:text="@string/btn_exit_activity_assignment1" />

    <Button
        android:id="@+id/btn_team_members"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btn_sudoku_game"
        android:layout_alignParentTop="true"
        android:text="@string/btn_team_member_activity_assignment1" />

</RelativeLayout>